<!--活动详情页面-->
<view>这是详情页面</view>
<!--轮播-->
  <swiper indicator-dots="{{indicatorDots}}"
          autoplay="{{autoplay}}" 
          interval="{{interval}}" 
          duration="{{duration}}"
          indicator-active-color="#36d9ba"
          circular="true"
          class="swiper"
          >
  <block wx:for="{{imgUrls}}" wx:key="">
    <swiper-item>
      <image src="{{item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>